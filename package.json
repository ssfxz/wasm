{
  "name": "NODE",
  "version": "1.0.0",
  "description": "NODE Client",
  "main": "./build/bundle.js",
  "scripts": {
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --colors --config config/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.config.prod.js",
    "wasm": "emcc -s WASM=1 -s SIDE_MODULE=1 -s ENVIRONMENT='web' src/cpp.c -Os -o lib/cpp.wasm"
  },
  "author": "Liuyang.61",
  "license": "MIT",
  "dependencies": {
    "@dp/bytecharts": "^1.0.0-mobile.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "html-webpack-plugin": "^4.3.0",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}

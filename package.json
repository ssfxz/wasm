{
  "name": "wasm",
  "version": "1.0.0",
  "description": "wasm demo",
  "main": "./build/bundle.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --colors --config config/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.config.prod.js",
    "wasm": "rm -rf ./lib & mkdir lib & emcc -s WASM=1 -s SIDE_MODULE=1 -s ENVIRONMENT='web' src/cpp.cpp -Os -o lib/cpp.wasm",
    "serve": "serve build -l 6600"
  },
  "author": "Liuyang.61",
  "license": "MIT",
  "dependencies": {
    "@dp/bytecharts": "^1.0.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "html-webpack-plugin": "^4.3.0",
    "serve": "^11.3.2",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}
